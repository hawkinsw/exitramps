{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "operator": {
          "type": "string"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "linkspeed": {
              "type": "object",
              "properties": {
                "in": {
                  "type": "integer"
                },
                "out": {
                  "type": "integer"
                }
              },
              "required": [
                "in",
                "out"
              ]
            }
          }
        },
        "config": {
          "type": "object",
          "properties": {
            "version": {
              "type": "integer"
            },
            "urls": {
              "type": "object",
              "properties": {
                "small_https_download_url": {
                  "type": "string"
                },
                "large_https_download_url": {
                  "type": "string"
                },
                "https_upload_url": {
                  "type": "string"
                },
                "small_download_url": {
                  "type": "string"
                },
                "large_download_url": {
                  "type": "string"
                },
                "upload_url": {
                  "type": "string"
                }
              },
              "required": [
                "small_https_download_url",
                "large_https_download_url",
                "https_upload_url",
                "small_download_url",
                "large_download_url",
                "upload_url"
              ]
            }
          },
          "required": [
            "version",
            "urls"
          ]
        }
      },
      "required": [
        "name",
        "operator",
        "latitude",
        "longitude",
        "config"
      ]
    }
  ]
}